<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.page.Mapper.UserMapper">
    <select id="findByPager" resultType="com.example.page.Entity.User">
        select * from User limit #{page},#{size}
    </select>
    <select id="findall" resultType="com.example.page.Entity.User">
        select * from User
    </select>
    <select id="count" resultType="long">
        select count(1) from User
    </select>
</mapper>